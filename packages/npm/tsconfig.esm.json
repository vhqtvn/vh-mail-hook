{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "module": "ESNext",
        "outDir": "./dist/esm",
        "moduleResolution": "node",
        "noEmit": false,
        "resolveJsonModule": true,
        "allowJs": true,
        "declaration": true,
        "declarationMap": true,
        "rewriteRelativeImportExtensions": true
    }
}
