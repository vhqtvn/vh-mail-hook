{
  "name": "@vhmailhook/client",
  "version": "1.0.0",
  "description": "TypeScript client library for VHMailHook service",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "repository": "github:vhqtvn/vh-mail-hook",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run build"
  },
  "keywords": [
    "email",
    "encryption",
    "age",
    "vhmailhook"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "age-encryption": "^0.2.0",
    "cross-fetch": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.17",
    "typescript": "^5.3.3"
  }
}
